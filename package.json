{
  "name": "@bryce-loskie/eslint-config",
  "version": "2.0.5",
  "packageManager": "pnpm@7.11.0",
  "description": "Opinionated ESLint configuration extended from @antfu and add more custom rules",
  "author": "guygubaby <guygubaby@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/guygubaby/eslint-config#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guygubaby/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/guygubaby/eslint-config/issues"
  },
  "keywords": [
    "eslint config"
  ],
  "main": "index.js",
